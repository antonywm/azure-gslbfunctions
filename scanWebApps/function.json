{
  "bindings": [
    {
      "name": "Timer",
      "schedule": "0 */1 * * * *",
      "direction": "in",
      "type": "timerTrigger"
    },
    {
      "name": "poolDataFile",
      "direction": "in",
      "type": "table",
      "connection": "AzureWebJobsStorage",
      "tableName": "poolData",
      "take": 50
    },
    {
      "name": "stateTable",
      "direction": "in",
      "type": "table",
      "connection": "AzureWebJobsStorage",
      "tableName": "stateTable",
      "take": 50
    }
  ]
}